#!/bin/sh
carthage checkout --use-ssh
carthage bootstrap --platform iOS --no-use-binaries
cd ../Carthage/Checkouts/XCoordinator           
swift package generate-xcodeproj
xcrun agvtool new-version -all 1
cd -
carthage build XCoordinator --platform iOS --no-use-binaries
# rm -rf ../Carthage/Checkouts/RxSwift/RxTest
# rm -rf ../Carthage/Checkouts/RxSwift/RxTest.podspec